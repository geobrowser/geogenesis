{
  "name": "geogenesis",
  "version": "1.0.0",
  "private": true,
  "description": "",
  "workspaces": [
    "packages/*",
    "apps/*"
  ],
  "scripts": {
    "test": "turbo run test",
    "build": "turbo run build",
    "codegen": "turbo run codegen",
    "lint": "turbo run lint",
    "format": "turbo run format",
    "dev": "turbo run dev",
    "clean": "turbo run clean && rimraf --glob \"**/.turbo\"",
    "docker:build": "docker compose build --build-arg TURBO_TEAM --build-arg TURBO_TOKEN",
    "docker:up": "docker compose up",
    "docker:down": "docker compose down -v --rmi all",
    "docker:clean": "rimraf build",
    "docker": "bun docker:down && bun docker:clean && bun docker:build && bun docker:up",
    "clean-node-modules": "rimraf --glob \"**/node_modules\""
  },
  "author": "Byron Guina",
  "license": "ISC",
  "prettier": {
    "singleQuote": true,
    "trailingComma": "es5",
    "semi": false
  },
  "devDependencies": {
    "@graphql-codegen/client-preset": "^5.2.3",
    "eslint": "^10.0.2",
    "eslint-config-turbo": "^2.8.11",
    "prettier": "^3.8.1",
    "rimraf": "^6.1.3",
    "turbo": "^2.8.11",
    "web": "workspace:*"
  },
  "overrides": {
    "viem": "^2.46.3"
  },
  "packageManager": "bun@1.3.9"
}
