{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "build": {
      "dependsOn": ["codegen", "^build"],
      "outputs": [".next/**", "dist/**", "build/**", "generated/**"]
    },
    "lint": {},
    "format": {},
    "codegen": {},
    "clean": {},
    "dev": {
      "dependsOn": ["build"],
      "outputs": [".next/**", "dist/**", "build/**"],
      "persistent": true,
      "cache": false
    },
    "test": {
      "dependsOn": ["build"]
    }
  }
}
