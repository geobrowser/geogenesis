{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["codegen", "^build"],
      "outputs": [
        ".next/**",
        "!.next/cache/**",
        "dist/**",
        "build/**",
        "generated/**"
      ],
      "cache": false
    },
    "lint": {
      "dependsOn": ["^lint"]
    },
    "format": {
      "dependsOn": ["^format"]
    },
    "codegen": {
      "dependsOn": ["^codegen"],
      "cache": false
    },
    "clean": {
      "dependsOn": ["^clean"]
    },
    "dev": {
      "dependsOn": ["^build"],
      "outputs": [".next/**", "dist/**", "build/**"]
    },
    "test": {
      "dependsOn": ["build"]
    },
    // "@geogenesis/action-schema#codegen": {
    //   "cache": false
    // },
    // "@geogenesis/action-schema#build": {
    //   "dependsOn": ["codegen"],
    //   "cache": false
    // },
    // "@geogenesis/contracts#build": {
    //   "dependsOn": ["@geogenesis/action-schema#build"],
    //   "outputs": ["dist/**", "build/**", "generated/**"],
    //   "cache": false
    // },
    // "web": {
    //   "dependsOn": ["@geogenesis/contracts#build"],
    //   "cache": false
    // },
    "@geogenesis/subgraph#codegen": {
      "dependsOn": ["@geogenesis/contracts#build"],
      "cache": false
    },
    "@geogenesis/subgraph#build": {
      "dependsOn": [
        "@geogenesis/subgraph#codegen",
        "@geogenesis/contracts#build"
      ],
      "cache": false
    },
    "deploy": {
      "dependsOn": ["build", "test", "lint", "format"],
      "outputs": []
    }
  }
}
