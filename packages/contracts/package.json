{
  "name": "@geogenesis/contracts",
  "description": "Smart contracts for the Geo app",
  "author": "Geo team",
  "license": "MIT",
  "version": "0.1.0",
  "files": [
    "dist/**/*",
    "addresses.json",
    "README.md",
    "LICENSE"
  ],
  "main": "index.js",
  "types": "index.d.ts",
  "scripts": {
    "preclean": "rm -rf build/ dist/",
    "clean": "npx hardhat clean",
    "precompile": "pnpm run clean",
    "compile": "npx hardhat compile",
    "build": "pnpm run compile && npx tsc",
    "devnet": "npx hardhat node",
    "deploy": "hardhat migrate",
    "deploy:local": "npx hardhat run --network localhost scripts/deploy.ts",
    "deploy:polygon:mainnet": "npx hardhat run --network polygon_mainnet scripts/deploy.ts",
    "deploy:polygon:mumbai": "npx hardhat run --network polygon_mumbai scripts/deploy.ts",
    "test": "npx hardhat test",
    "ganache": "ganache",
    "test:integration": "npx hardhat run --network localhost test:integration/index.test.ts",
    "test:report-gas": "REPORT_GAS=true npx hardhat test",
    "test:coverage": "npx hardhat coverage",
    "prepublishOnly": "npx tsc"
  },
  "devDependencies": {
    "@ethersproject/abi": "^5.6.4",
    "@ethersproject/bytes": "^5.6.1",
    "@ethersproject/providers": "^5.6.8",
    "@geogenesis/fact-schema": "workspace:^0.0.1",
    "@nomiclabs/hardhat-ethers": "^2.0.6",
    "@nomiclabs/hardhat-etherscan": "^3.0.3",
    "@nomiclabs/hardhat-waffle": "^2.0.3",
    "@openzeppelin/contracts": "^4.6.0",
    "@typechain/ethers-v5": "^10.0.0",
    "@typechain/hardhat": "^6.0.0",
    "@types/app-root-path": "^1.2.4",
    "@types/chai": "^4.3.1",
    "@types/express": "^4.17.13",
    "@types/lodash.set": "^4.3.7",
    "@types/mocha": "^9.1.1",
    "@types/node": "^17.0.34",
    "@typescript-eslint/eslint-plugin": "^5.25.0",
    "@typescript-eslint/parser": "^5.25.0",
    "app-root-path": "^3.0.0",
    "chai": "^4.3.6",
    "dotenv": "^16.0.1",
    "eslint": "^8.15.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.2.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "ethereum-waffle": "^3.4.4",
    "ethers": "^5.6.6",
    "ganache": "^7.1.0",
    "hardhat": "^2.9.5",
    "hardhat-abi-exporter": "^2.9.0",
    "hardhat-gas-reporter": "^1.0.8",
    "nanoid": "^3.3.4",
    "prettier": "^2.6.2",
    "prettier-plugin-solidity": "^1.0.0-beta.19",
    "solhint": "^3.3.7",
    "solidity-coverage": "^0.7.21",
    "ts-node": "^10.7.0",
    "typechain": "^8.0.0",
    "typescript": "4.6.4"
  },
  "engines": {
    "node": "18"
  },
  "dependencies": {
    "lodash.set": "^4.3.2"
  }
}
