    "@bufbuild/protoc-gen-es": "^1.9.0",
    "@connectrpc/connect": "^1.3.0",
    "@connectrpc/connect-node": "^1.3.0",
    "@effect/schema": "^0.63.0",
    "@geogenesis/sdk": "workspace:*",
    "@sentry/node": "^7.112.2",
    "@substreams/core": "^0.16.0",
    "@substreams/manifest": "^0.15.0",
    "@substreams/node": "^0.6.2",
    "@substreams/sink": "^0.14.0",
    "@types/md5": "^2.3.5",
    "commander": "latest",
    "dotenv": "latest",
    "effect": "^2.4.19",
    "fast-check": "3.13.2",
    "md5": "^2.3.0",
    "pg": "^8.7.3",
    "pg-cursor": "^2.10.3",
    "prom-client": "latest",
    "tsx": "^4.6.0",
    "uuid": "^9.0.0",
    "viem": "^1.16.0",
    "yaml": "^2.4.0",
    "zapatos": "^6.2.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@trivago/prettier-plugin-sort-imports": "^4.2.0",
    "@types/node": "latest",
    "@types/pg": "^8.10.9",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "dotenv-cli": "^7.3.0",
    "esbuild": "^0.19.5",
    "eslint": "^8.21.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^3.5.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.0.0",
    "prettier": "^3.0.3",
