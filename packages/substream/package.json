{
  "name": "@geogenesis/substream",
  "version": "0.0.1",
  "description": "Geo Substreams Sink",
  "exports": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "license": "MIT OR Apache-2.0",
  "scripts": {
    "build": "esbuild index.ts --bundle --outfile=dist/index.js --bundle --platform=node",
    "prepublishOnly": "npm run build",
    "nuke-public": "tsx ./src/utils/executeSql.ts ./src/sql/nukePublic.sql",
    "zapatos": "pnpm dotenv -e .env -- npx zapatos",
    "start:from-genesis": "pnpm dotenv -e .env -- node dist/index.js --from-genesis",
    "start:from-genesis-with-cache": "pnpm dotenv -e .env -- node dist/index.js --from-cache",
    "start": "pnpm dotenv -e .env -- node dist/index.js",
    "initialize": "./src/sql/initAll.sh && pnpm zapatos"
  },
  "dependencies": {
    "@bufbuild/buf": "^1.28.0",
    "@bufbuild/protobuf": "^1.4.2",
    "@connectrpc/connect": "^1.1.3",
    "@connectrpc/connect-node": "^1.1.3",
    "@effect/platform-node": "^0.31.9",
    "@effect/schema": "^0.48.4",
    "@substreams/core": "^0.8.0",
    "@substreams/manifest": "^0.7.0",
    "@substreams/node": "^0.4.3",
    "@substreams/sink": "^0.6.0",
    "commander": "latest",
    "dotenv": "latest",
    "effect": "^2.0.0-next.55",
    "fast-check": "^3.13.2",
    "fetch-retry": "^5.0.6",
    "node-fetch": "^3.3.2",
    "pg": "^8.7.3",
    "pg-cursor": "^2.10.3",
    "prom-client": "latest",
    "tslog": "latest",
    "tsx": "^4.6.0",
    "yaml": "^2.3.4",
    "zapatos": "^6.2.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@trivago/prettier-plugin-sort-imports": "^4.2.0",
    "@types/node": "latest",
    "@types/pg": "^8.10.9",
    "@typescript-eslint/eslint-plugin": "^5.46.0",
    "@typescript-eslint/parser": "^6.0.0",
    "dotenv-cli": "^7.3.0",
    "esbuild": "^0.19.5",
    "eslint": "^8.21.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^3.5.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.0.0",
    "prettier": "^3.0.3",
    "typescript": "latest"
  },
  "packageManager": "pnpm@7.30.5"
}
